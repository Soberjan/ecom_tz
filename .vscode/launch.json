  "version": "0.2.0",
  "configurations": [
      {
          "name": "Python File",
          "type": "debugpy",
          "request": "launch",
          "program": "${file}",
          "console": "integratedTerminal",
          "env": {
              "PYTHONPATH": "${workspaceFolder}/src/"
          },
          "cwd": "${workspaceFolder}"
      },
      {
          "name": "django_w10",
          "type": "debugpy",
          "request": "launch",
          "program": "${workspaceFolder}/src/examples/w10/manage.py",
          "console": "integratedTerminal",
          "justMyCode": false,
          "args": ["runserver"],
          "env": {
              "PYTHONPATH": "${workspaceFolder}/src/"
          },
          "cwd": "${workspaceFolder}"
      },
      {
          "name": "migrate",
          "type": "debugpy",
          "request": "launch",
          "program": "${workspaceFolder}/src/examples/w10/manage.py",
          "console": "integratedTerminal",
          "justMyCode": false,
          "args": ["migrate"],
          "env": {
              "PYTHONPATH": "${workspaceFolder}/src/"
          },
          "cwd": "${workspaceFolder}"
      },
      {
          "name": "django_real",
          "type": "debugpy",
          "request": "launch",
          "program": "${workspaceFolder}/django_real/manage.py",
          "console": "integratedTerminal",
          "justMyCode": false,
          "args": ["runserver"],
          "env": {
              "PYTHONPATH": "${workspaceFolder}/django_real"
          },
          "cwd": "${workspaceFolder}"
      },
      {
          "name": "migrate_real",
          "type": "debugpy",
          "request": "launch",
          "program": "${workspaceFolder}/django_real/manage.py",
          "console": "integratedTerminal",
          "justMyCode": false,
          "args": ["migrate"],
          "env": {
              "PYTHONPATH": "${workspaceFolder}/django_real"
          },
          "cwd": "${workspaceFolder}"
      },
      {
          "name": "django_migrations",
          "type": "debugpy",
          "request": "launch",
          "program": "${workspaceFolder}/src/examples/w10/manage.py",
          "console": "integratedTerminal",
          "justMyCode": false,
          "args": ["makemigrations", "app2"],
          "env": {
              "PYTHONPATH": "${workspaceFolder}/src/"
          },
          "cwd": "${workspaceFolder}"
      }
  ]
}